{
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:@typescript-eslint/recommended"],
  "rules": {
    "@typescript-eslint/no-non-null-assertion": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "no-debugger": "error",
    "no-duplicate-imports": "warn",
    "no-dupe-keys": "error",
    "no-console": "error"
  }
}
